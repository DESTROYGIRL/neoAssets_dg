"UnlitGeneric"
{
	"$basetexture" "vgui\hud\jgr\jgr_active"
	"$translucent" 1
	"$vertexcolor" 1
	"$vertexalpha" 1
	"$no_fullbright" 1
	"$additive" 1
	"$ignorez" 1
 "Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" 0.5
      }
   }
}
